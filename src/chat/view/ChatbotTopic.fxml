<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="background" stylesheets="@ChatbotTopic.css" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="chat.view.ChatbotTopicController">
   <children>
      <TableView layoutX="75.0" layoutY="148.0" prefHeight="300.0" prefWidth="450.0">
        <columns>
          <TableColumn prefWidth="75.0" text="Topic" />
          <TableColumn prefWidth="75.0" text="Item" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Label fx:id="TopicLabel" layoutX="228.0" layoutY="66.0" text="Topic List" />
      <Button fx:id="aboutButton" layoutX="499.0" layoutY="20.0" mnemonicParsing="false" onAction="#handleAbout" text="About" />
      <Button layoutX="404.0" layoutY="20.0" mnemonicParsing="false" onAction="#handleHelpButton" text="Help" />
   </children>
</AnchorPane>
